{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP into an HA Pair",
    "Common": {
        "class": "Tenant",
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "${nameserver}"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org","1.pool.ntp.org","2.pool.ntp.org"
            ],
            "timezone": "UTC"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "asm": "nominal"
        },
        "external": {
            "class": "VLAN",
            "tag": 4092,
            "mtu": 1460,
            "interfaces": [
                {
                    "name": "1.0",
                    "tagged": false
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 4093,
            "mtu": 1460,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.30.0.10/32",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "external-self": {
            "class": "SelfIp",
            "address": "10.20.0.9/32",
            "vlan": "external",
            "allowService": [
                "tcp:443"
            ],
            "trafficGroup": "traffic-group-local-only"
        },

        "ext_gw": {
            "class": "Route",
            "target": "external",
            "network": "10.20.0.1/32",
            "mtu": 1460
        },
        "ext_rt": {
            "class": "Route",
            "gw": "10.20.0.1",
            "network": "10.20.0.0/24",
            "mtu": 1460
        },
        "int_gw": {
            "class": "Route",
            "target": "internal",
            "network": "10.30.0.1/32",
            "mtu": 1460
        },
        "int_rt": {
            "class": "Route",
            "gw": "10.30.0.1",
            "network": "10.30.0.0/24",
            "mtu": 1460
        },
        "default": {
            "class": "Route",
            "gw": "10.20.0.1",
            "network": "default",
            "mtu": 1460
        }
    }
}