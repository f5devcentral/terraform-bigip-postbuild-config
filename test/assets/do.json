{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP",
    "Common": {
        "class": "Tenant",
        "myDns": {
            "class": "DNS",
            "nameServers": ${jsonencode(nameserver)},
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org","1.pool.ntp.org","2.pool.ntp.org"
            ],
            "timezone": "UTC"
        },
        "myProvisioning": {
            %{ for service in provision }
            "${service}": "nominal",
            %{ endfor }
            "class": "Provision"
        },
        "external": {
            "class": "VLAN",
            "tag": "${external_vlan_tag}",
            "mtu": "${mtu_size}",
            "interfaces": [
                {
                    "name": "${external_vlan_name}",
                    "tagged": false
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": "${internal_vlan_tag}",
            "mtu": "${mtu_size}",
            "interfaces": [
                {
                    "name": "${internal_vlan_name}",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "${internal_selfip_address}",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "external-self": {
            "class": "SelfIp",
            "address": "${external_selfip_address}",
            "vlan": "external",
            "allowService": [
                "tcp:443"
            ],
            "trafficGroup": "traffic-group-local-only"
        }
    }
}