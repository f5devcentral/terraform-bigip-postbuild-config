{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP",
    "Common": {
        "class": "Tenant",
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "${nameserver}"
            ],
            "search": [
                "f5.com"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org","1.pool.ntp.org","2.pool.ntp.org"
            ],
            "timezone": "UTC"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal",
            "asm": "nominal"
        },
        "external": {
            "class": "VLAN",
            "tag": 10,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 20,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.30.0.10/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "external-self": {
            "class": "SelfIp",
            "address": "10.20.0.9/24",
            "vlan": "external",
            "allowService": [
                "tcp:443"
            ],
            "trafficGroup": "traffic-group-local-only"
        },
        "newGRE": {
            "class": "Tunnel",
            "tunnelType": "gre",
            "mtu": 2000,
            "usePmtu": false,
            "typeOfService": 125,
            "autoLastHop": "default",
            "key": 0,
            "localAddress": "${tunnel_local_address}",
            "remoteAddress": "${tunnel_remote_address}",
            "secondaryAddress": "any6",
            "mode": "bidirectional",
            "transparent": false,
            "trafficGroup": "none"
        }
    }
}